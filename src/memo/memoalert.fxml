<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="border" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="90.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="memo.memoalertController">
   <top>
      <Label fx:id="lblText" alignment="CENTER" contentDisplay="TOP" prefHeight="57.0" prefWidth="400.0" text="변경내용을 저장하시겠습니까?" BorderPane.alignment="CENTER">
         <font>
            <Font name="휴먼고딕" size="20.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
   </top>
   <bottom>
      <HBox fx:id="hbox" alignment="CENTER_RIGHT" prefHeight="10.0" prefWidth="400.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnSave" mnemonicParsing="false" prefHeight="20.0" prefWidth="70.0" text="저장" />
            <Button fx:id="btnNonSave" mnemonicParsing="false" onAction="#NonSave" prefHeight="20.0" prefWidth="70.0" text="저장 안함" />
            <Button fx:id="btnClose" mnemonicParsing="false" onAction="#Cancel" prefHeight="20.0" prefWidth="70.0" text="취소" />
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
